    <!-- Ryu Movelist -->
    <string name="ryu_shoryuken_description">Startup invulnerability on MP and EX versions.</string>
    <string name="ryu_vtrigger_description">Powers up specials and gains ability to charge fireballs. Fully charged fireballs are unblockable.</string>
    <string name="ryu_vskill_description">Parries an attack and builds some V-meter.</string>

    <!-- Karin Movelist -->
    <string name="karin_tenko_posttext">Press Punch immediately to perform enhanced version.</string>
    <string name="karin_meioken_posttext">Hold buttons to change properties</string>
    <string name="karin_vskill_description">Destroys projectiles with a palm strike.</string>
    <string name="karin_vtrigger_description">Karin gains access to Guren Ken, a new special move with a number of followups.</string>

    <!-- Claw Movelist -->
    <string name="claw_claw_off_only">(Claw Off Only)</string>
    <string name="claw_claw_on_only">(Claw On Only)</string>
    <string name="claw_switch">Switch to Claw On/Off</string>
    <string name="claw_vtrigger_torero_description">Throws a rose straight forward. On hit or block, performs a follow-up attack.</string>
    <string name="claw_vtrigger_rojo_description">Throws a rose at an upward angle. On hit or block, performs a follow-up attack.</string>
    <string name="claw_vtrigger_azul_description">Throws a rose at a downward angle. On hit or block, performs a follow-up attack.</string>

    <!-- Chun Movelist -->
    <string name="chun_up_to_three_times">Up to 3 Times</string>
    <string name="chun_vtrigger_description">Adds additional hits to most normals, which extends blockstun and hitstun and introduces new combo possibilities.</string>
    <string name="chun_vskill_description">A diagonal jump which launches opponents on hit.</string>
    <string name="chun_senenshu_description">Hits overhead. Leads to combo in V-Trigger.</string>
    <string name="chun_critical_art_description">Projectile invulnerable.</string>

    <!-- Ken Movelist -->
    <string name="ken_vtrigger_description">Moves Ken forward, maintaining pressure or extending a combo. Afterward, Ken\'s special moves are faster and gain better hitboxes.</string>
    <string name="ken_vskill_description">Moves Ken forward. Holding the buttons performs a step kick.</string>
    <string name="ken_thunder_kick_posttext">Tap to feint, hold to attack</string>

    <!-- Zangief Movelist -->
    <string name="zangief_hold_buttons_for_special_lariat">Hold Buttons to Perform Special Lariat</string>
    <string name="zangief_can_move_forward_while_holding_buttons">Can Move Forward While Holding Buttons</string>
    <string name="zangief_vtrigger_description">Vacuums opponent toward Zangief. By holding the buttons, Zangief will continue attacking and the lariat will gain hitboxes. V-Skill gains infinite armor.</string>
    <string name="zangief_vskill_description">Zangief gains one hit of armor (infinite hits in V-Trigger). Zangief attacks when the buttons are released.</string>

    <!--Dictator Movelist -->
    <string name="dictator_psycho_reflect_hold_buttons">Hold Buttons to Throw Back a Projectile</string>
    <string name="dictator_after_head_press">(After Head Press Hits)</string>
    <string name="dictator_before_head_press">(Before Head Press Hits)</string>
    <string name="dictator_devil_reverse_description">Crosses up while in V-Trigger</string>
    <string name="dictator_vskill_description">Press buttons to absorb a projectile. Hold buttons to throw a projectile back.</string>
    <string name="dictator_vtrigger_description">Special moves become cancellable into EX moves. Various special move properties change. Dashes gain ability to cross up.</string>

    <!-- Birdie Movelist -->
    <string name="birdie_hanging_chain_posttext">Chain Twirl: Hold Kick\nCancel: Press Punch</string>
    <string name="birdie_vtrigger_description">Birdie deals more damage. Special moves become faster, multi-hitting, or gain armor. </string>

    <!-- Mika Movelist -->
    <string name="mika_typhoon_description">Leaves Mika behind the opponent.</string>
    <string name="mika_brimstone_description">Leaves Mika in front of the opponent.</string>
    <string name="mika_vtrigger_posttext">Hold buttons to delay Nadeshiko</string>
    <string name="mika_vskill_posttext">Hold buttons to extend speech</string>
    <string name="mika_vskill_description">Mika delivers a speech, ending with a mic spike which has a projectile hitbox. The speech powers up command throws more the longer it goes on.</string>

    <!-- Rashid Movelist -->
    <string name="rashid_repeat_buttons_to_extend">Repeat Button Presses to Extend</string>
    <string name="rashid_vtrigger_description">Creates a tornado that locks the opponent down and changes the trajectory of Rashid\'s dash, jump, and attacks.</string>
    <string name="rashid_vskill_divekick_pretext">(During Front Flip)</string>
    <string name="rashid_vskill_rollkick_pretext">(During Rolling Assault)</string>
    <string name="rashid_whirlwind_shot_description">HP and EX versions cancel into V-Skill and affect Rashid\'s trajectory.</string>

    <!-- Nash Movelist -->
    <string name="nash_press_for_second_boom">Press Punch During EX Version For A Second Projectile</string>
    <string name="nash_vskill_description">A chop attack which can absorb a projectile, building V-Meter.</string>

    <!-- Necalli Movelist -->
    <string name="necalli_vskill_control_distance">Control Distance Using Direction</string>
    <string name="necalli_disc_guidance_description">Has one hit of armor against projectiles.</string>
    <string name="necalli_vtrigger_description">Necalli gains improved frame advantage, more range on Raging Light, and no longer requires charge to cancel into Disc\'s Guidance.</string>
    <string name="necalli_vskill_description">Builds V-Meter on hit.</string>
    <string name="necalli_opening_dagger_description">Hits overhead. Crush counters.</string>
    <string name="necalli_raging_light_description">Invulnerability on HP and EX versions.</string>

    <!-- Laura Movelist -->
    <string name="laura_after_mp_or_ex_bolt_charge">(After MP or EX Bolt Charge)</string>
    <string name="laura_hold_buttons_to_power_up">Hold Buttons to Power Up</string>
    <string name="laura_bolt_charge_description">MP and EX versions can be followed up.\nHP version anti-airs.</string>
    <string name="laura_split_river_description">Leaves Laura behind opponent.</string>
    <string name="laura_rodeo_break_description">Leaves Laura in front of opponent.</string>
    <string name="laura_vtrigger_description">Adds stun and damage to Laura\'s attacks. Adds distance to V-Skill dash and increases hitstun and blockstun on V-Skill overhead.</string>
    <string name="laura_vskill_overhead_description">Hits overhead.</string>
    <string name="vskill_avante_description">Moves Laura forward.</string>
    <string name="vskill_esquiva_description">Moves Laura backward. Can be cancelled into forward V-Skill.</string>

    <!-- Cammy Movelist -->
    <string name="cammy_during_hooligan">(During Hooligan Combination)</string>
    <string name="cammy_vtrigger_description">Makes specials faster and safer.</string>
    <string name="cammy_vskill_description">Moves Cammy forward with projectile invulnerability. Can combo on hit.</string>

    <!-- Dhalsim Movelist -->
    <string name="dhalsim_critical_art_posttext">Hold button to charge</string>
    <string name="dhalsim_vtrigger_description">Places fire on the ground that deals damage over time to opponents.</string>
    <string name="dhalsim_vskill_description">Dhalsim floats. Duration can be shortened by performing an attack.</string>

    <!-- Fang Movelist -->
    <string name="fang_vskill_description">Shoots a projectile forward that applies poison if it touches the opponent. This attack causes no hitstun or blockstun.</string>
    <string name="fang_critical_art_description">He turns into a bird.</string>

</resources>
